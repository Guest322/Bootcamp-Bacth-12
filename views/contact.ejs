    <h1>Ini Adalah Page Contact</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Phone Number</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% cont.forEach(contact => { %>
                <tr>
                    <td><%= contact.name  %></td>
                    <td><%= contact.phone %></td>
                    <td><%= contact.mail %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
